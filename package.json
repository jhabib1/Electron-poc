{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "electron.js",
  "build": {
    "appId": "com.electron.humanitix",
    "directories": {},
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "NSIS"
    }
  },
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"webpack --watch\" \"electron .\"",
    "build": "rimraf ./public && mkdir public && webpack --progress -p",
    "package-current": "electron-builder",
    "package-all": "electron-builder -mw"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "concurrently": "3.4.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "electron": "1.4.1",
    "electron-builder": "^20.27.1",
    "electron-packager": "^12.1.0",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "file-loader": "0.10.0",
    "html-webpack-plugin": "2.28.0",
    "postcss": "5.2.12",
    "postcss-cssnext": "2.9.0",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.3.0",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.2.1"
  },
  "dependencies": {
    "antd": "^3.8.2",
    "axios": "0.15.3",
    "body-parser": "^1.18.3",
    "classnames": "2.2.5",
    "dotenv": "4.0.0",
    "express": "^4.16.3",
    "mongoose": "^5.2.7",
    "morgan": "^1.9.0",
    "react": "15.4.2",
    "react-activity": "^1.2.2",
    "react-dom": "15.4.2",
    "react-router": "3.0.2",
    "react-router-dom": "^4.3.1",
    "reqwest": "^2.0.5"
  }
}
